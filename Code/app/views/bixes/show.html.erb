<p>Foi criado um novo bixe:</p>
  <% b_id = @bixe.id %>
  <%  @bixe_has_user = BixeHasUser.find_by_bixe_id(b_id) %>
  <%  u_id = @bixe_has_user.user_id %>
  <%  @user = User.find(u_id) %>
 
  <p>Email:        <%= @user.email %></p>
  <p>Nome:         <%= @bixe.name %></p>
  <p>Telefone:     <%= @bixe.telephone %></p>
  <p>Curso:        <%= @bixe.course %></p>
  <p>Tutoria:      <%= @bixe.tutor %></p>
  <p>Modalidades:  <%= @bixe.sports %></p>